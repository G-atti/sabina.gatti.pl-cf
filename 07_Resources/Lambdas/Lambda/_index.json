{
	"LambdaNewsletterSubscription": {
		"Type": "AWS::Lambda::Function",
		"Properties": {
			"FunctionName": "sabina_gatti_pl_newsletter_subscribe",
			"Code": {
				"ZipFile": "exports.handler = async (event) => {return true;};"
			},
			"Handler": "index.handler",
			"MemorySize": 128,
			"Role": { "Fn::GetAtt": ["LambdaNewsletterSubscriptionRole", "Arn"] },
			"Runtime": "nodejs8.10",
			"Timeout": 60,
			"Environment": {
				"Variables" : {
					"MAILCHIMP_API_KEY": { "Ref": "MailchimpAPIKey" },
					"MAILCHIMP_LIST_ID": { "Ref": "MailchimpListID" }
				}
			}
		}
	}
}
